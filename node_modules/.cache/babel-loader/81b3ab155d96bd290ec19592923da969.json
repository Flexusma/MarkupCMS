G,CAAC,IAJH;AAKd,gBAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;;AALc,sBAMX,MAAM,KAAG,SAAT,IAAoB,EAAE,MAAK,YAAa,KAApB,CANT;AAAA;AAAA;AAAA;;AAOZ,gBAAA,KAAI,CAAC,SAAL,GAAiB,MAAM,CAAC,IAAxB;AACA,gBAAA,KAAI,CAAC,SAAL,GAAiB,MAAM,CAAC,IAAxB;;AARY,sBAST,MAAM,CAAC,KAAP,KAAe,SAAf,IAA0B,MAAM,CAAC,KAAP,KAAe,IAThC;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAUJ,KAAI,CAAC,UAAL,CAAgB,MAAM,CAAC,KAAvB,CAVI;;AAAA;AAAA;;AAAA;AAchB,gBAAA,KAAI,CAAC,SAAL,GAAe,MAAf;AACA,gBAAA,KAAI,CAAC,SAAL,GAAe,aAAf;;AAfgB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBjB,KAlBK;AAmBA,IAAA,UAnBA,sBAmBW,EAnBX,EAmBc;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACF,YAAY,CAAC,cAAb,CAA4B,EAA5B,CADE;;AAAA;AACd,gBAAA,GADc;AAElB,gBAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;AACA,gBAAA,MAAI,CAAC,WAAL,GAAiB,GAAG,CAAC,IAAJ,CAAS,GAA1B;;AAHkB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIpB;AAvBM;AAjBK,CAAf","sourcesContent":["<template>\r\n  <div class=\"author-container m-0 p-3 container-fluid\">\r\n    <div class=\"\">\r\n      <div class=\"author-display p-3 align-items-center row\">\r\n        <div class=\"h-auto col-auto p-1 author-img-cont rounded-circle\">\r\n          <img class=\"author-img rounded-circle\" :src=\"picture_url\" />\r\n        </div>\r\n        <div class=\"author-box col\">\r\n\r\n            <h3 v-if=\"opt_name&&opt_name!==''\">{{opt_name}}</h3>\r\n            <h3 v-else>{{disp_name}}</h3>\r\n            <p v-if=\"opt_desc&&opt_desc!==''\" class=\"mt-3 mb-0\">{{opt_desc}}</p>\r\n            <p v-else>{{disp_desc}}</p>\r\n\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport AuthorService from \"@/services/AuthorService\";\r\nimport ImageService from \"@/services/ImageService\";\r\n\r\nexport default {\r\nname: \"AuthorDisplay\",\r\n  props:[\"id\", \"opt_name\",\"opt_desc\", \"opt_img\"],\r\n  data(){\r\n  this.getDetails();\r\n  if(this.opt_img!==undefined) this.getPicture(this.opt_img);\r\n    return{\r\n      disp_name:\"Name\",\r\n      disp_desc:\"Description\",\r\n      picture_url:\"https://lunamedia.de/wp-content/uploads/2016/03/blank-profile-picture-973460_960_720.png\"\r\n    }\r\n  },\r\n  watch:{\r\n    opt_name: function() {this.getDetails();},\r\n    opt_desc: function() {this.getDetails();},\r\n    opt_img: function () {this.getPicture(this.opt_img)}\r\n  },\r\n  methods:{\r\n\r\n    async getDetails(){\r\n      console.log(this.id,this.opt_name,this.opt_desc)\r\n      if(this.id!==undefined){\r\n        let res = await AuthorService.getAuthorByID(this.id);\r\n        let author = res.data;\r\n        console.log(res)\r\n        if(author!==undefined&&!(author instanceof Error)) {\r\n          this.disp_name = author.name;\r\n          this.disp_desc = author.desc;\r\n          if(author.pp_id!==undefined&&author.pp_id!==null)\r\n            await this.getPicture(author.pp_id);\r\n          return;\r\n        }\r\n      }\r\n      this.disp_name=\"Name\";\r\n      this.disp_desc=\"Description\"\r\n    },\r\n    async getPicture(id){\r\n      let res = await ImageService.getImageFromID(id);\r\n      console.log(res);\r\n      this.picture_url=res.data.url;\r\n    }\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n\r\n.author-container{\r\n  background: #A50DFF;\r\n}\r\n.author-img{\r\n  max-height: 200px;\r\n  max-width: 200px;\r\n  min-width: 50px;\r\n  min-height: 50px;\r\n  width: 20vw;\r\n  height: 20vw;\r\n  object-fit: cover;\r\n}\r\n.author-img-cont{\r\n  background: white;\r\n}\r\ninput[type=submit]{\r\n  margin: 0;\r\n}\r\n.author-container{\r\n  color: #fff;\r\n}\r\n</style>"],"sourceRoot":""}]}